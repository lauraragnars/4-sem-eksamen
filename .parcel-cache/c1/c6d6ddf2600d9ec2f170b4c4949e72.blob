* {
  box-sizing: border-box;
}

body,
h1,
h2,
h3,
h4,
h5,
h6 {
  margin: 0;
  padding: 0;
}

ol,
ul {
  list-style: none;
}

img {
  width: 100%;
}

@font-face {
  font-family: "pilowlavaa";
  src: url("Pilowlava-Regular.HASH_REF_64428cd3d61c828e5d4f3a2a98d2a764.woff2") format("woff2"), url("Pilowlava-Regular.HASH_REF_921646b1c182119e729a538a3035b012.woff") format("woff");
}

.burger-menu-svg {
  width: 4rem;
  position: fixed;
  right: 1.5rem;
  top: 1.5rem;
  mix-blend-mode: difference;
  transition: all 0.3s ease-out;
}

header .open #blob-9 {
  fill: #f2c0ff;
  mix-blend-mode: normal;
}

header .open .burger-menu-text {
  opacity: 0;
  transition: 0.3s;
}

header .open .burger-menu-svg {
  width: 180vw;
  top: -100vh;
  right: -40vw;
  transition: all 0.3s ease-in;
  fill: #f2c0ff;
  mix-blend-mode: normal;
}

header .open .burger-nav {
  display: block;
}

.burger-nav a {
  display: none;
  text-decoration: none;
  color: black;
  font-family: fenwick, sans-serif;
  font-weight: 400;
  font-style: normal;
  position: fixed;
  font-size: 6rem;
}

.burger-menu-text {
  font-family: fenwick, sans-serif;
  font-weight: 400;
  font-style: normal;
  position: fixed;
  right: 2.3rem;
  top: 3.3rem;
  font-size: 0.7rem;
  transition: 0.3s;
}

.burger-menu {
  cursor: pointer;
}

.header-logo {
  background-blend-mode: difference;
  position: fixed;
  top: 1rem;
  left: 1.5rem;
}

