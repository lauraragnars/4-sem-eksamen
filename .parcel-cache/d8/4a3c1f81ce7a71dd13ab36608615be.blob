* {
  box-sizing: border-box;
}

body,
h1,
h2,
h3,
h4,
h5,
h6 {
  margin: 0;
  padding: 0;
}

ol,
ul {
  list-style: none;
}

img {
  width: 100%;
}

@font-face {
  font-family: "pilowlavaa";
  src: url("ad806f5f86aa5ae5bf1abcbae461b2b6") format("woff2"), url("b3fa1625893a29e34f3deadb20fc359d") format("woff");
}

#index-body {
  background-color: #8000ff;
  background-image: url("53bee693d05fba49c48a256981953b90");
  background-size: cover;
  background-repeat: no-repeat;
  background-blend-mode: overlay;
}

#program-body {
  background-color: #7979ff;
