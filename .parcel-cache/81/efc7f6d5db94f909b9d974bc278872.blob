endTo(elem);\n//   }.bind(this)\n// );\n// }\n\ndocument.fonts.ready.then(function () {\n  const elem = document.querySelector(\".splash-text\");\n  const text = new Blotter.Text(\"DISTORTION Ø\", {\n    family: \"pilowlava-regular\",\n    weight: 100,\n    size: 150,\n    fill: \"white\",\n  });\n\n  let material = new Blotter.RollingDistortMaterial();\n\n  material.uniforms.uSineDistortAmplitude.value = 0.04;\n\n  let blotter = new Blotter(material, {\n    texts: text,\n  });\n\n  let scope = blotter.forText(text);\n\n  scope.appendTo(elem);\n});\n"],"names":["document","font","FontFace","Promise","all","load","then","loadedFonts","forEach","fonts","add","$d3dfc027196df876626c47bef188877c$var$showData","data","console","log","querySelector","textContent","fields","aboutHeader","aboutText","window","addEventListener","url","$d3dfc027196df876626c47bef188877c$var$link","callback","fetch","response","json","jsonData","countDownDate","Date","getTime","setInterval","now","timeleft","days","Math","floor","hours","minutes","seconds","$d3dfc027196df876626c47bef188877c$var$countdown","ready","elem","text","Blotter","Text","family","weight","size","fill","material","RollingDistortMaterial","uniforms","uSineDistortAmplitude","value","blotter","texts","forText","appendTo"],"version":3,"file":"index.HASH_REF_4ac83920e3d1b41a8be7270675b40076.js.map"}