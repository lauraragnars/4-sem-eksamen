* {
  box-sizing: border-box;
}

body,
h1,
h2,
h3,
h4,
h5,
h6 {
  margin: 0;
  padding: 0;
}

ol,
ul {
  list-style: none;
}

img {
  width: 100%;
}

@font-face {
  font-family: "pilowlavaa";
  src: url("ad806f5f86aa5ae5bf1abcbae461b2b6") format("woff2"), url("b3fa1625893a29e34f3deadb20fc359d") format("woff");
}

.burger-menu-svg {
  width: 4rem;
  position: fixed;
  right: 1.5rem;
  top: 1.5rem;
  mix-blend-mode: difference;
  transition: all 0.3s ease-out;
}

header .open .burger-menu-svg {
  width: 180vw;
  top: -100vh;
  right: -40vw;
  transition: all 0.3s ease-in;
  fill: #f2c0ff;
  mix-blend-mode: normal;
}

header .open #blob-9 {
  fill: #f2c0ff;
  mix-blend-mode: normal;
}

header .open .burger-menu-text {
  opacity: 0;
  transition: 0.3s;
}

.burger-menu-text {
  font-family: fenwick, sans-serif;
  font-weight: 400;
  font-style: normal;
  position: fixed;
  right: 2.3rem;
  top: 3.3rem;
  font-size: 0.7rem;
  transition: 0.3s;
}

.burger-menu {
  cursor: pointer;
}

.header-logo {
  background-blend-mode: difference;
  position: fixed;
  top: 1rem;
  left: 1.5rem;
}

body {
  min-height: 100vh;
  background-size: cover;
  background-repeat: no-repeat;
  background-blend-mode: overlay;
}

#index-body {
  background-color: #8000ff;
  background-image: url("4d70035a99ffbcd11444334abc898542");
}

#program-body {
  background-color: #7979ff;
  background-image: url("4108fc813a64306e4912fa4c39ba4a38");
}

#merch-body {
  background-color: #835fff;
}

#practical-info-body {
  background-color: #4a53e8;
}

#artists-body {
  background-color: #b29bff;
  background-image: url("c837baf1b60af0dcbd2a0d04b38c7551");
}

#tickets-body {
  background-color: #f2c0ff;
}

#map-body {
  background-color: #835fff;
}