"use strict";

const link =
  "https://cdn.contentful.com/spaces/hor06eo7sbnb/environments/master/entries/7k6leOigNpIaVOJ3Nf9Mrb?access_token=v4LMdVINCbVKD5EiT3xd7HTxYBdDedDAw_naZMWSOVQ";

window.addEventListener("load", start);

function start() {
  console.log("start");

  // show data at load
  loadJSON(link, showData);
}

// loads data
function loadJSON(url, callback) {
  fetch(url)
    .then((response) => response.json())
    .then((jsonData) => {
      callback(jsonData);
    });
}

function showData(data) {
  console.log(data);
}
