* {
  box-sizing: border-box;
}

body,
h1,
h2,
h3,
h4,
h5,
h6,
p {
  margin: 0;
  padding: 0;
}

ol,
ul {
  list-style: none;
}

img,
video {
  width: 100%;
}

button {
  border: none;
}

.burger-menu-svg {
  mix-blend-mode: difference;
  width: 4rem;
  position: fixed;
  right: 1.5rem;
  top: 1.5rem;
  transition: 0.3s ease-out;
  z-index: 3;
}

.burger-nav {
  position: fixed;
  z-index: 2;
  transition: all 0.3s;
  background-color: #f2c0ff;
  opacity: 0;
  pointer-events: none;
  top: 0;
  left: 0;
  height: 100vh;
  width: 100vw;
  padding-top: 150px;
  text-align: center;
}

.burger-nav li {
  display: block;
}

.burger-nav ul {
  padding-left: 0;
}

.burger-nav a {
  display: none;
  text-decoration: none;
  color: white;
  font-family: fenwick, sans-serif;
  font-weight: 400;
  font-style: normal;
  font-size: 3rem;
  transition: 0.3s;
  line-height: 60px;
  display: inline-block;
}

@media (min-width: 768px) {
  .burger-nav a {
    font-size: 6rem;
    line-height: 110px;
  }
}

.burger-nav a:hover {
  color: black;
  transition: 0.3s;
}

header .open .burger-nav {
  opacity: 1;
  pointer-events: all;
  transition: all 0.3s;
}

.burger-menu-text {
  font-family: fenwick, sans-serif;
  font-weight: 400;
  font-style: normal;
  position: fixed;
  right: 2.3rem;
  top: 3.3rem;
  z-index: 4;
  font-size: 0.7rem;
  transition: 0.3s;
  color: white;
  mix-blend-mode: difference;
}

.burger-menu-icon {
  cursor: pointer;
}

.header-logo {
  mix-blend-mode: difference;
  position: fixed;
  top: 1rem;
  left: 1.5rem;
}

footer {
  width: 100vw;
  height: 330px;
  background-image: url("628b2f5e41d434edc531d01e69b0aa51");
  background-repeat: no-repeat;
  background-size: 150%;
  background-position-x: center;
}

footer .footer-logo {
  position: relative;
  bottom: -180px;
  left: 40px;
}

footer .footer-logo img {
  width: 90px;
}

.body-container {
  min-height: 100vh;
  width: 100vw;
  background-size: cover;
  background-repeat: no-repeat;
  background-blend-mode: overlay;
  overflow-x: hidden;
}

#index-body {
  background-color: #8000ff;
  background-image: url("4d70035a99ffbcd11444334abc898542");
}

#program-body {
  background-color: #7979ff;
  background-image: url("4108fc813a64306e4912fa4c39ba4a38");
}

#merch-body {
  background-color: #835fff;
}

#practical-info-body {
  background-color: #4a53e8;
}

#artists-body {
  background-color: #b29bff;
  background-image: url("7b2b8bbd6ea91b283e0be255225a4780");
  background-size: 150%;
  background-position-x: center;
}

#single-artist-body {
  background-color: #b29bff;
  background-image: url("7b2b8bbd6ea91b283e0be255225a4780");
  background-size: 150%;
  background-position-x: center;
}

#tickets-body {
  background-color: #f2c0ff;
}

#map-body {
  background-color: #835fff;
}

.section-wrapper {
  width: 70vw;
  margin-left: 15vw;
}

h1 {
  font-family: fenwick, sans-serif;
  font-size: 3rem;
  font-weight: 400;
  font-style: normal;
  text-align: center;
  padding-top: 150px;
  padding-bottom: 50px;
}

@media (min-width: 768px) {
  h1 {
    font-size: 5rem;
    text-align: left;
    padding: 90px 250px 20px;
  }
}

.button {
  font-family: fenwick, sans-serif;
  font-weight: 300;
  font-style: normal;
  color: white;
  background-color: black;
  padding: 12px 22px;
  font-size: 0.7rem;
  text-decoration: none;
}

* {
  cursor: none;
}

.custom-cursor {
  position: absolute;
  width: 20px;
  height: 20px;
  background: white;
  z-index: 8;
  border-radius: 50%;
  mix-blend-mode: difference;
  pointer-events: none;
  transition: transform 0.15s ease-in-out;
}

#index-body .splash-text {
  text-align: center;
  margin: 150px 0 0 0;
}

#index-body .pink-banner {
  background-image: url("017be3c1c9b252a5420e8b26d063b445");
  width: 100vw;
  height: 450px;
  background-size: cover;
  background-repeat: no-repeat;
}

#index-body .video-section {
  margin: 200px 0;
}

#index-body .about-text-header {
  font-family: fenwick, sans-serif;
  font-weight: 400;
  font-style: normal;
  font-size: 0.84rem;
}

@media (min-width: 768px) {
  #index-body .about-text-header {
    font-size: 1rem;
  }
}

#index-body .about-text {
  font-family: fenwick, sans-serif;
  font-weight: 400;
  font-style: normal;
  font-size: 1.2rem;
  padding: 10px 0 30px 0;
}

@media (min-width: 768px) {
  #index-body .about-text {
    max-width: 50vw;
    font-size: 1.4rem;
  }
}

#index-body .about-section {
  padding-top: 100px;
}

@media (min-width: 768px) {
  #index-body .about-section {
    padding-top: 130px;
  }
}

#index-body .countdown-section {
  text-align: center;
}

#index-body .countdown {
  display: flex;
  justify-content: center;
  margin: 150px 0;
}

#index-body .countdown p {
  font-family: fenwick, sans-serif;
  font-weight: 400;
  font-style: normal;
  font-size: 6rem;
  color: white;
}

#index-body .cta-section {
  display: flex;
  margin: 0 auto;
  justify-content: center;
  align-content: center;
  margin-bottom: 150px;
}

#index-body .cta-section a {
  text-decoration: none;
}

#index-body .cta-section .blob-1 {
  background-image: url("801e1b699b709b8b907db3408f98cd9a");
  background-size: cover;
  background-repeat: no-repeat;
  width: 150px;
  height: 132px;
  margin: 0 10px;
}

#index-body .cta-section p {
  text-decoration: none;
  color: black;
  font-family: fenwick, sans-serif;
  font-weight: 400;
  font-style: normal;
  text-align: center;
  padding-top: 45px;
}

#index-body .cta-section .blob-2 {
  background-image: url("ea965234d9db3a946463577028091a2b");
  background-size: cover;
  background-repeat: no-repeat;
  width: 150px;
  height: 132px;
  margin: 0 10px;
}

#artists-body #menu {
  fill: black;
}

#artists-body .white-banner-top {
  background-image: url("2fd69e79c7b2fe522a8d0dffa35eca14");
  background-size: 100%;
  background-repeat: no-repeat;
  background-position: top;
  width: 100vw;
  height: 3vw;
}

#artists-body .white-banner-bottom {
  background-image: url("e5489cc5c1d99f6066557facd39bc585");
  background-size: 100%;
  background-repeat: no-repeat;
  background-position: top;
  width: 100vw;
  height: 5vw;
}

#artists-body .white-banner-content {
  background-color: white;
  padding: 100px 0;
  width: 100vw;
  height: 80vh;
}

#artists-body .button-container {
  display: flex;
  flex-direction: row;
  justify-content: center;
}

#artists-body button {
  background-color: white;
  color: black;
  border: solid black 1px;
  margin: 0 10px;
  min-width: 120px;
}

#artists-body .selected {
  background-color: black;
  color: white;
}

#artists-body button:active {
  background-color: black;
  color: white;
}

#artists-body #all-artists {
  text-align: center;
  margin: 50px 0;
  padding: 0;
  overflow-y: scroll;
  height: 60vh;
}

#artists-body #all-artists li {
  font-family: fenwick, sans-serif;
  font-size: 1.5rem;
  font-weight: 400;
  font-style: normal;
  margin: 10px 0;
}

@media (min-width: 768px) {
  #artists-body #all-artists li {
    font-size: 2.5rem;
  }
}

#artists-body #all-artists .headliner {
  font-size: 2.5rem;
}

@media (min-width: 768px) {
  #artists-body #all-artists .headliner {
    font-size: 4rem;
  }
}