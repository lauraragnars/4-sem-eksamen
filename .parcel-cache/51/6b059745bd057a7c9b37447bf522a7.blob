* {
  box-sizing: border-box;
}

body,
h1,
h2,
h3,
h4,
h5,
h6 {
  margin: 0;
  padding: 0;
}

ol,
ul {
  list-style: none;
}

img {
  width: 100%;
}

@font-face {
  font-family: "pilowlavaa";
  src: url("6858e7f6b7f15040eb7f19d238bce03f") format("woff2"), url("538b02930f83e91093ab6c219a6202de") format("woff");
}

#index-body {
  background-color: #8000ff;
  background-image: url("4123a06ed645e5588f4dace7a482e691");
  background-size: cover;
  background-repeat: no-repeat;
  background-blend-mode: overlay;
}

