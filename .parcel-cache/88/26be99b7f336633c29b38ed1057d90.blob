* {
  box-sizing: border-box;
}

body,
h1,
h2,
h3,
h4,
h5,
h6,
p {
  margin: 0;
  padding: 0;
}

ol,
ul {
  list-style: none;
}

img,
video {
  width: 100%;
}

.burger-menu-svg {
  mix-blend-mode: difference;
  width: 4rem;
  position: fixed;
  right: 1.5rem;
  top: 1.5rem;
  transition: 0.3s ease-out;
  z-index: 3;
}

.burger-nav {
  position: fixed;
  z-index: 2;
  transition: all 0.3s;
  background-color: #f2c0ff;
  opacity: 0;
  pointer-events: none;
  top: 0;
  left: 0;
  height: 100vh;
  width: 100vw;
}

.burger-nav li {
  display: block;
}

.burger-nav a {
  display: none;
  text-decoration: none;
  color: white;
  font-family: fenwick, sans-serif;
  font-weight: 400;
  font-style: normal;
  font-size: 3rem;
  transition: 0.3s;
  display: inline-block;
}

.burger-nav a:hover {
  color: black;
  transition: 0.3s;
}

header .open .burger-nav {
  opacity: 1;
  pointer-events: all;
  transition: all 0.3s;
}

.burger-menu-text {
  font-family: fenwick, sans-serif;
  font-weight: 400;
  font-style: normal;
  position: fixed;
  right: 2.3rem;
  top: 3.3rem;
  z-index: 4;
  font-size: 0.7rem;
  transition: 0.3s;
  color: white;
  mix-blend-mode: difference;
}

.burger-menu-icon {
  cursor: pointer;
}

.header-logo {
  background-blend-mode: difference;
  position: fixed;
  top: 1rem;
  left: 1.5rem;
}

footer {
  width: 100vw;
  height: 330px;
  background-image: url("footer_blob.HASH_REF_45ee5353450ff4c4309c83977c05421b.svg");
  background-repeat: no-repeat;
  background-size: 150%;
  background-position-x: center;
}

footer .footer-logo {
  position: relative;
  bottom: -180px;
  left: 40px;
}

footer .footer-logo img {
  width: 90px;
}

body {
  min-height: 100vh;
  background-size: cover;
  background-repeat: no-repeat;
  background-blend-mode: overlay;
  overflow-x: hidden;
}

